{
  "name": "tamia",
  "description": "Tiny but extremely opinionated frontend framework",
  "version": "5.0.0-alpha.4",
  "scripts": {
    "pretest": "npm run lint",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "prepublishOnly": "npm run compile",
    "lint": "eslint . --cache --fix --ext .tsx",
    "typecheck": "tsc --noEmit",
    "compile": "npm run compile:babel && npm run compile:types",
    "compile:babel": "babel --delete-dir-on-start --extensions '.tsx' -d lib/ src/",
    "compile:types": "tsc --emitDeclarationOnly",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy": "gh-pages -d styleguide/public",
    "docs": "npm run styleguide:build && npm run deploy",
    "format": "prettier --write '**/*.{tsx,md}'"
  },
  "homepage": "http://tamiadev.github.io/tamia/",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tamiadev/tamia.git"
  },
  "bugs": {
    "url": "https://github.com/tamiadev/tamia/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.134",
    "core-js": "^3.1.4",
    "csstype": "^2.6.5",
    "lodash": "^4.17.11",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "styled-system": "^5.0.10"
  },
  "peerDependencies": {
    "styled-components": ">=4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "styled-components": "^4.1.16",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-tamia": "^6.2.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-docgen-typescript": "^1.12.5",
    "react-dom": "^16.8.6",
    "react-js-to-ts": "^1.4.0",
    "react-styleguidist": "9.1.7",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0"
  }
}
