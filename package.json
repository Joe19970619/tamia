{
  "name": "tamia",
  "description": "Tiny but extremely opinionated frontend framework",
  "version": "4.0.0-alpha.12",
  "scripts": {
    "test": "npm run lint",
    "prepublishOnly": "npm run compile",
    "lint": "eslint . --fix",
    "compile": "babel --delete-dir-on-start -d lib/ src/",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "deploy": "gh-pages -d styleguide/public",
    "docs": "npm run styleguide:build && npm run deploy",
    "format": "prettier --write '**/*.{js,md}'",
    "posttest": "npm run format"
  },
  "homepage": "http://tamiadev.github.io/tamia/",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me/"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tamiadev/tamia.git"
  },
  "bugs": {
    "url": "https://github.com/tamiadev/tamia/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "lodash": "^4.17.10",
    "polished": "^1.9.3",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "emotion": ">=9",
    "emotion-theming": ">=9",
    "react-emotion": ">=9"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-flow": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-polished": "^1.1.0",
    "emotion": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "eslint": "^5.2.0",
    "eslint-config-tamia": "^6.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^1.2.0",
    "prettier": "^1.14.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.6",
    "react-styleguidist": "^7.1.3",
    "webpack": "^4.16.3",
    "webpack-blocks": "^2.0.0-alpha"
  }
}
